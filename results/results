aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  45)     """
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  46) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  47)     # Flask debug mode
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  48)     DEBUG = False
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  49)     TESTING = False
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  50) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  51)     # Secret key needs to be randomly generated (you can use os.urandom(24))
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  52)     SECRET_KEY = os.environ.get(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  53)         'SECRET_KEY',
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  54)         '\xa6TQ5\xc9\xf942\x9cx\x9b\xed\xa4\xc7\x95\xcc\xfd\xb8Q\xa1\x80\x99Z%')  # noqa
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  55)     # Separate token for CSRF protection (by default it uses secret key)
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  56)     # Random data for generating secure tokens.
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  57)     WTF_CSRF_SECRET_KEY = os.environ.get(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  58)         'WTF_CSRF_SECRET_KEY',
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  59)         '\xc9\xcc\x91{\xd9\x9a\x18\x92\xaa\xb4\x9e\x80\x07\x13\x92-\x1ciH\x86\xecz:[')  # noqa
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  60) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  61)     # Turn off Flask-SQLAlchemy event system
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  62)     SQLALCHEMY_TRACK_MODIFICATIONS = False
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  63) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  64)     # SQLAlchemy database location
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  65)     # PostgreSQL - postgresql://username:password@hostname/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  66)     # MySQL - mysql://username:password@hostname/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  67)     # SQLite - sqlite:////absolute/path/to/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  68)     # Create the default db/sqlite directory if it doesn't exist
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  69)     # Avoid sqlalchemy.exc.OperationalError: (sqlite3.OperationalError)
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  70)     if not os.path.isdir('db/sqlite'):
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  71)         os.makedirs('db/sqlite')
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  72)     SQLALCHEMY_DATABASE_URI = os.environ.get(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  73)         'DATABASE_URL',
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  74)         'sqlite:///' + os.path.join(basedir, 'db/sqlite/myapp.db'))
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  75) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  76)     # Flask-OAuthlib - OAuth 2.0 Credentials
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  77)     # https://developers.google.com/api-client-library/python/guide/aaa_client_secrets
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  78)     google_client_id, google_client_secret = get_secrets_from_json(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  79)         provider_name='google',
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  80)         filename=os.path.join(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  81)             basedir, 'myapp/oauth2/google_client_secrets.json'))
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  82)     facebook_app_id, facebook_app_secret = get_secrets_from_json(
