aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 146)     FLASK_COVERAGE = True
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 147)     # Setting a SERVER_NAME also by default enables URL generation without
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 148)     # a request context but with an application context.
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 149)     SERVER_NAME = 'localhost:5000'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 150)     # Disable CSRF protection in the testing configuration
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 151)     WTF_CSRF_ENABLED = False
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 152)     # For testing we can use an in memory database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 153)     SQLALCHEMY_DATABASE_URI = 'sqlite://'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 154)     # OAuth 2.0 - OpenID connect
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 155)     # OAuthLib will raise an InsecureTransportError if you attempt to use
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 156)     # OAuth2 over HTTP, rather than HTTPS. This is mostly useful for local
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 157)     # testing, or automated tests. Never set these in production!
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 158)     # Prevent OAuth 2 MUST utilize https on Development / Test server
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 159)     # OAUTHLIB_INSECURE_TRANSPORT = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 160)     # OAUTHLIB_RELAX_TOKEN_SCOPE = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 161)     # DEBUG = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 162)     # os.environ['DEBUG'] = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 163) 
