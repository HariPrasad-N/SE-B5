aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  84)         filename=os.path.join(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  85)             basedir, 'myapp/oauth2/facebook_client_secrets.json'))
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  86)     # We use the same names for our OAUTH2_PROVIDERS as used by Flask-OAuthlib
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  87)     # http://flask-oauthlib.readthedocs.io/en/latest/api.html
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  88)     OAUTH2_PROVIDERS = {
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  89)         'google': {
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  90)             'client_id': google_client_id,
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  91)             'client_secret': google_client_secret
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  92)         },
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  93)         'facebook': {
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  94)             'client_id': facebook_app_id,
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  95)             'client_secret': facebook_app_secret
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  96)         }
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  97)     }
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  98)     # print(OAUTH2_PROVIDERS)
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530  99) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 100) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 101) class DevelopmentConfig(BaseConfig):
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 102)     """
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 103)     DevelopmentConfig holds development configuration for myapp
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 104)     """
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 105)     # Flask debug mode
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 106)     DEBUG = True
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 107)     # Flask-DebugToolbar - Disable intercept redirects
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 108)     DEBUG_TB_INTERCEPT_REDIRECTS = False
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 109)     # SQLAlchemy database location
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 110)     # PostgreSQL - postgresql://username:password@hostname/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 111)     # MySQL - mysql://username:password@hostname/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 112)     # SQLite - sqlite:////absolute/path/to/database
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 113)     SQLALCHEMY_DATABASE_URI = os.environ.get(
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 114)         'DATABASE_DEV_URL',
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 115)         'sqlite:///' + os.path.join(basedir, 'db/sqlite/dev_myapp.db'))
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 116)     # OAuth 2.0 - OpenID connect
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 117)     # OAuthLib will raise an InsecureTransportError if you attempt to use
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 118)     # OAuth2 over HTTP, rather than HTTPS. This is mostly useful for local
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 119)     # testing, or automated tests. Never set this variable in production.
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 120)     # Prevent OAuth 2 MUST utilize https on Development / Test server
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 121)     # OAUTHLIB_INSECURE_TRANSPORT = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 122)     # OAUTHLIB_RELAX_TOKEN_SCOPE = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 123)     # DEBUG = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 124)     # os.environ['DEBUG'] = '1'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 125) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 126) 
