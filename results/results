aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 130)     """
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 131)     # SQLAlchemy database location: keep the default sqlite
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 132)     # Flask-Login - Session Protection
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 133)     SESSION_PROTECTION = 'strong'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 134)     # Run publicly - app.run(host='0.0.0.0')
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 135)     HOST = '0.0.0.0'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 136)     # Change the listening port - app.run(port=5001)
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 137)     PORT = '5001'
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 138) 
aad2b3d7 (ganesh 2018-11-08 08:36:21 +0530 139) 
